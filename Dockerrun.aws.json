{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions": [
        {
            "name": "backend",
            "image": "512324380783.dkr.ecr.us-east-1.amazonaws.com/radgab_backend",
            "essential": true,
            "memory": 512
        },
        {
            "name": "frontend",
            "image": "512324380783.dkr.ecr.us-east-1.amazonaws.com/radgab_frontend",
            "essential": true,
            "memory": 128,
            "portMappings": [
                {
                    "hostPort": 80,
                    "containerPort": 80
                }
            ],
            "links": [
                "backend"
            ]
        }
    ]
}